class Solution {
    public int busyStudent(int[] s, int[] e, int q) {
        int count=0;
        for(int i=0;i<s.length;i++){
            for(int j=s[i];j<=e[i];j++){
                if(j==q){
                    count++;
                }
            }
        }
        return count;
        
    }
}
